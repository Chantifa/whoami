{
  "name": "whoami",
  "version": "1.0.0",
  "description": "This project is based on a GitLab [Project Template](https://docs.gitlab.com/ee/gitlab-basics/create-project.html).",
  "main": "server.js",
  "type": "module",
  "dependencies": {
    "bcrypt": "5.1.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.8.2",
    "socket.io": "^4.5.4",
    "swagger-ui-express": "^4.6.0",
    "prom-client": "^14.1.1"
  },
  "scripts": {
    "start": "nodemon server.js",
    "start-client": "npm --prefix ./client --openssl-legacy-provider start",
    "start-all": "npm --openssl-legacy-provider start & npm run --openssl-legacy-provider start-client && fg",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://git.ffhs.ch/ramona.koksa/whoami.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "babel-jest": "^26.6.0",
    "jest": "26.6.0",
    "nodemon": "^2.0.20",
    "react-testing-library": "^8.0.1",
    "yamljs": "^0.3.0"
  },
  "nodemonConfig": {
    "ignore": [
      "client/src/*.js"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "mjs",
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/client/"
    ],
    "transform": {
      "^.+\\.m?js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "\\.pnp\\.[^\\/]+$"
    ]
  }
}
